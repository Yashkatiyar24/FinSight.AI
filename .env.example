# FinSight.AI Environment Variables
# Copy this file to .env.local and fill in your values

# Supabase Configuration
# FinSight.AI Environment Configuration

# Supabase Configuration (Required)
VITE_SUPABASE_URL=your_supabase_project_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# OpenAI Configuration (Optional - for AI categorization)
# If not provided, the app will use rule-based categorization
VITE_OPENAI_API_KEY=your_openai_api_key_here

# Development Configuration
VITE_APP_ENV=development

# Example URLs:
# VITE_SUPABASE_URL=https://your-project.supabase.co
# VITE_SUPABASE_ANON_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9...
# VITE_OPENAI_API_KEY=sk-...

# Server-side Supabase (for API routes and Edge Functions)
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Razorpay Configuration
VITE_RAZORPAY_KEY_ID=your_razorpay_key_id
RAZORPAY_KEY_SECRET=your_razorpay_key_secret

# Security
NEXTAUTH_SECRET=your_32_character_secret_key
ENCRYPTION_SECRET_32B=your_32_byte_encryption_secret

# API Configuration
VITE_API_BASE_URL=http://localhost:3001

# Development
NODE_ENV=development
